// ********************************************************************** 
function InfoAni(){
	//document.getElementById("obap").innerHTML = '3';
	if (InfoM=="on"){ $("#pimm-tausta").animate({top: '-1%'}, 1).animate({top: '100%'}, 10000); InfoAni();};  //document.getElementById("pimm-tausta").style.Top="0px";
};
// **********************************************************************
function SleepTimerCheck(){ SleepTimer();
	var tslet = document.getElementById("sleepss").value;
	if(isNaN(tslet) && tslet == 0){} else {SleepTimer()};
};
// **********************************************************************
function SleepTimer(){
	var tslet = document.getElementById("sleepss").value;
	var tsletn = Number(tslet);
	var nn = tsletn.toString();
	if (tsletn <= 9999 && tsletn >= 1){SleepTimerOon(tslet); } else {document.getElementById("sleepss").value=""};
};
// **********************************************************************
function SleepTimerOon(tslet){
	$("#sleepT").animate({width: '0px'}, 300);
	document.getElementById("ajastin").innerHTML = tslet;
	$("#ajastin").animate({right: '0px'}, 1000);
	var oo = tslet;
	var SleepVar = setTimeout(function(){ SleepTimerC(oo); }, 1000);
};
// **********************************************************************
function SleepTimerC(oo){
	var ooo = Number(oo);
	ooo = ooo - 1;
    document.getElementById("ajastin").innerHTML = ooo;
	//var styles = 'width:100%; font-size:15em; transform:rotate(20deg)';
//styles;
	if (ooo <= 0){document.getElementById("slPimennys").style.zIndex = '20'; document.getElementById("slPimennys").style.width= '100%'; document.getElementById("slPimennys").style.height= '100%';$("#ajastin").animate({right: '-360px'}, 1000); $("#slPimennys").animate({opacity: '1'}, 20000); SleepTAnimat();} else {if (SleepTimeri == "on"){var SleepVar = setTimeout(function(){ SleepTimerC(ooo); }, 1000);};};
};
// **********************************************************************    
function SleepTAnimat(){
player=player3;
  currentVolume = player.getVolume();
  if (currentVolume === 0) {
    player.pauseVideo();
    player.unMute(); } else {
    player.setVolume(currentVolume - 3);
    return setTimeout(function(){SleepTAnimat(player);}, 1000);
};
};	
// **********************************************************************
function FullScriin(){
var elem = document.getElementById("body");
if (elem.requestFullscreen) {elem.requestFullscreen();
} else if (elem.mozRequestFullScreen) {
elem.mozRequestFullScreen();
} else if (elem.webkitRequestFullscreen) {
elem.webkitRequestFullscreen();
}
};
// **********************************************************************
function exitFullscreen() {
  if(document.exitFullscreen) {
    document.exitFullscreen();
  } else if(document.mozCancelFullScreen) {
    document.mozCancelFullScreen();
  } else if(document.webkitExitFullscreen) {
    document.webkitExitFullscreen();
  }
};
// **********************************************************************
function Bigger(){
	$("#vnaytto, #vnaytto1, #vnaytto2").stop(true);
	
	if (player3==player1){document.getElementById("vnaytto2").style.left= 720+Xsize+"px";} else {document.getElementById("vnaytto1").style.left= 720+Xsize+"px";};
	
	document.getElementById("vnaytto").style.height= 500+Ysize+"px";
	document.getElementById("vnaytto").style.marginTop= -250-(Ysize/2)+"px";
	document.getElementById("vnaytto").style.width= 710+Xsize+"px";
	document.getElementById("vnaytto").style.marginLeft= -360-(Xsize/2)+"px";
	document.getElementById("vnaytto1").style.height= 480+Ysize+"px";
	document.getElementById("vnaytto1").style.width= 720+Xsize+"px";
	document.getElementById("vnaytto2").style.height= 480+Ysize+"px";
	document.getElementById("vnaytto2").style.width= 720+Xsize+"px";
	
	player2.setSize(width=720+Xsize, height=405+Ysize);
	player1.setSize(width=720+Xsize, height=405+Ysize);
		
	NappiSmallerr.id="NappiSmaller"; NappiBiggerr.id="NappiBigger";
	
}; 
// **********************************************************************
function StarBigger(){
	$("#vnaytto, #vnaytto1, #vnaytto2").stop(true);
	
	//if (player3==player1){document.getElementById("vnaytto2").style.left= 720+Xsize+"px";} else {document.getElementById("vnaytto1").style.left= 720+Xsize+"px";};
	
	document.getElementById("vnaytto").style.height= 500+Ysize+"px";
	document.getElementById("vnaytto").style.marginTop= -250-(Ysize/2)+"px";
	document.getElementById("vnaytto").style.width= 710+Xsize+"px";
	document.getElementById("vnaytto").style.marginLeft= -360-(Xsize/2)+"px";
	document.getElementById("vnaytto1").style.height= 480+Ysize+"px";
	document.getElementById("vnaytto1").style.width= 720+Xsize+"px";
	document.getElementById("vnaytto2").style.height= 480+Ysize+"px";
	document.getElementById("vnaytto2").style.width= 720+Xsize+"px";
	
	document.getElementById("menu").style.top= 96+(Ysize/2)+"px";
	document.getElementById("menu").style.left= 180+(Xsize/2)+"px";
	
	player2.setSize(width=720+Xsize, height=405+Ysize);
	player1.setSize(width=720+Xsize, height=405+Ysize);
		
	NappiSmallerr.id="NappiSmaller"; NappiBiggerr.id="NappiBigger";
	
}; 
// ******************TAPAHTUMA KUUNTELU****************************************************
function onPlayerState(event){
	toisto = event.target.getPlayerState();
	if (toisto==1){Vpla="oon"; NappiPlay.id="NappiPlayPause"; document.getElementById("NappiPlayPause").src ="../images/ButtonsSPaus_03.png"; }; //play
	if (toisto==2){Vpla="ooff"; NappiPlayPause.id="NappiPlay"; document.getElementById("NappiPlay").src ="../images/Buttons_03.png"; }//player3.pauseVideo(); }; //pause 
	//if (toisto==5){Vpla="oon"; NappiPlay.id="NappiPlayPause"; document.getElementById("NappiPlayPause").src ="../images/ButtonsSPaus_03.png"; };
	
	//if (toisto==3){Vpla="ooff"; NappiPlayPause.id="NappiPlay"; document.getElementById("NappiPlay").src ="../images/Buttons_03.png"; RModee();};
	if (toisto==0){Vpla="ooff"; NappiPlayPause.id="NappiPlay"; document.getElementById("NappiPlay").src ="../images/Buttons_03.png"; RModee();}; 
}; 
// ******************REPEAT MODE****************************************************
function RModee(){ 
	if (RMode==0){Vpla="ooff";};
	if (RMode==1){AutoP="On"; VSiirto(); };
	if (RMode==2){AutoP="On"; VNro=VNro+1; VSiirto(); };
	if (RMode==3){AutoP="On"; VNro=Math.floor(Math.random() * VName.length); VSiirto(); };
};
// *******************ANIMATION***************************************************
function VSiirtoAni() {
	Vpla="ooff"; 
	
	$("#sllPimennys").animate({opacity: '1'}, 4000);
	
	$("#flogos, #donationFlogos").stop(true);
	$("#flogos, #donationFlogos").animate({opacity: '0'}, 8000); //, function(){window.clearTimeout(t);}); 
	$("#about").animate({opacity: '0.95'}, 1010);
	//$("#about").animate({opacity: '0.15'}, 1000);
	$("#NappiPlayA").animate({opacity: '0'}, 1000);
	//$("#ylat").stop(false); 
	$("#ylat").stop(true); $("#ylat").animate({opacity: '1'}, 1600);
	$("#soitin").animate({top: '62px'}, 1200);
	$("#menu").animate({left:'640px'}, 5000);
	VSiirto();
};
// *******************TUNE SIIRTO***************************************************
function VSiirto() {
	//window.clearTimeout(myAuto);
 
	VMaara=VName.length;
	if (VNro==-1){VNro=VMaara-1};
	if (VNro==VMaara){VNro=0};
	if (player3==player1){player3=player2; player3.loadVideoById(VName[VNro]).stopVideo(); $("#vnaytto1").animate({left:725+Xsize+"px"}, 2000); $("#vnaytto2").animate({left:'0px'}, 2000); } else {player3=player1; player3.loadVideoById(VName[VNro]).stopVideo(); $("#vnaytto2").animate({left:725+Xsize+"px"}, 2000); $("#vnaytto1").animate({left:'0px'}, 2000); };
	if (AutoP=="Off"){ Vpla="ooff";  };
	if (AutoP=="On"){ player3.playVideo(); AutoP="Off"; };
};
// **********************************************************************
function VASiirto() {
	
	AutoP="On";
	VSiirtoAni();
};
// ******************MENU ANIMATION****************************************************
function VMenusiirto() {
	Vpla="ooff"; 
	//var NappinImg = 
	if (player3==player1){ $("#vnaytto1").animate({left:720+Xsize+"px"}, 1200); } else {$("#vnaytto2").animate({left:720+Xsize+"px"}, 1200); };
	
	//var disp= document.getElementById("NappiPlay");
  //if (window.getComputedStyle(disp).display === "none") {document.getElementById("NappiPlayPause").id="NappiPlay"; document.getElementById("NappiPlay").src ="../images/Buttons_03.png"; } else {document.getElementById("NappiPlay").id="NappiPlay"; document.getElementById("NappiPlayPause").src ="../images/Buttons_03.png"; };
  
	//if (toisto==1) {document.getElementById("NappiPlayPause").id="NappiPlay"; document.getElementById("NappiPlay").src ="../images/Buttons_03.png"; };
	$("#page-background, #flogos, #donationFlogos").stop(true);  
	$("#sllPimennys").stop(true);
	$("#sllPimennys").animate({opacity: '0'}, 1000);
	document.body.style.cursor = "default";
	
	$("#about, #NappiPlayA").animate({opacity: '1'}, 400);
	//$("#page-background").animate({opacity: '0.9'}, 1000); // ----------------------------------------------------------------------------
	$("#flogos, #donationFlogos").animate({opacity: '1'}, 5000);
	$("#ylat").animate({opacity: '0'}, 1000);
	$("#soitin").animate({top: '-90px'}, 800);
	document.getElementById("menu").style.top= 96+(Ysize/2)+"px";
	$("#menu").stop(true); $("#menu").animate({left:180+(Xsize/2)+'px'}, 820);
	player3.pauseVideo().stopVideo();
	/* $("#pimennys").animate({height: '0%'}, 500)  document.getElementById("menu").style.Left= 180+(Xsize/2)+'px';
	$("#InfoOverU").animate({width: '0%'}, 200) */
};
