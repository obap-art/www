// ******************************* KIELI CLICK *************************************************************
function kieli(ob) { 
	var Nappi= ob.id;
	
	if (Nappi=="langeng") {kielieng()};
	if (Nappi=="langfin") {kielifin()};
}; 
function kielieng() { 
	document.getElementById("langeng").src ="../images/fin.png"; langeng.id="langfin";
	document.getElementById("iteksti").src ="../images/info_sketz1_eng.png";
	document.getElementById("tekeilla").src ="../images/logoinfo_eng.png";
	document.getElementById("obaplogo").src ="../images/obap-weblogo-eng.png"; 



}; 
function kielifin() { 
	document.getElementById("langfin").src ="../images/eng_11.png"; langfin.id="langeng";
	document.getElementById("iteksti").src ="../images/info_sketz1.png";
	document.getElementById("tekeilla").src ="../images/logoinfo_fin.png";
	document.getElementById("obaplogo").src ="../images/obap-weblogo-fin.png"; 

}; 
// ****************************** SOITIN NAPPI ROLL OVERI ****************************************
function sOveri(ob) { 
	var Nappi= ob.id;
	//Vpla="ooff";
	//if (myAuto) {window.clearTimeout(myAuto);};
	document.getElementById(Nappi).className="btoo";
	document.getElementById(Nappi).style.cursor = "pointer";
}; 
// ******************************* SOITIN NAPPI ROLL OUTTI ***************************************
function sOutti(ob) { 
	var Nappi= ob.id;
	//Vpla="oon";
	//if (myAuto) {window.clearTimeout(myAuto);};
	document.getElementById(Nappi).className="bto";
}; 
// ****************************** INFO NAPPI ROLL OVERI ****************************************
function InfoOveri(ob) { 
	var Nappi= ob.id;
	document.getElementById(Nappi).style.cursor = "pointer";
}; 
