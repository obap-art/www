function Overi(ob) { 
	var Nappi= ob.id;
	//if (myAuto) {window.clearTimeout(myAuto);};
	if (Nappi=="NappiMenu") {document.getElementById(Nappi).style.cursor = "pointer"; };
	if (Nappi=="NappiFullS") {document.getElementById(Nappi).style.cursor = "pointer"; };
	if (Nappi=="NappiFullD") {document.getElementById(Nappi).style.cursor = "pointer";};  
	if (Nappi=="NappiSmaller") {document.getElementById(Nappi).style.cursor = "pointer"; };
	if (Nappi=="NappiBigger") {document.getElementById(Nappi).style.cursor = "pointer"; };
	
	if (Nappi=="NappiPlayA") {document.getElementById(Nappi).style.cursor = "pointer"; };
	if (Nappi=="NappiSleepOk") {document.getElementById(Nappi).style.cursor = "pointer"; };
	
	
	if (Nappi=="NappiBack") {document.getElementById(Nappi).src ="../images/Buttons_11.png"; document.getElementById(Nappi).style.cursor = "pointer"; };
	if (Nappi=="NappiForward") {document.getElementById(Nappi).src ="../images/Buttons_12.png"; document.getElementById(Nappi).style.cursor = "pointer"; }; 
	if (Nappi=="NappiPlay") {document.getElementById(Nappi).src ="../images/Buttons_03.png"; document.getElementById(Nappi).style.cursor = "pointer"; };
	if (Nappi=="NappiPlayPause") {document.getElementById(Nappi).src ="../images/ButtonsSPaus_03.png"; document.getElementById(Nappi).style.cursor = "pointer"; };
	
// **********
	
	if (Nappi=="NappiNoba") {document.getElementById(Nappi).style.cursor = "pointer"; $("#NobaOverU").animate({width: '100%', left: '0%'}, 200);};
	if (Nappi=="NappiAmbsu") {document.getElementById(Nappi).style.cursor = "pointer"; $("#AmbsuOverU, #AmbsuOverUU").animate({height: '26'}, 200);};
	if (Nappi=="NappiKansi") {document.getElementById(Nappi).style.cursor = "pointer"; $("#KansiOverU").animate({width: '100%', left: '0%'}, 200);};
	if (Nappi=="NappiTwo") {document.getElementById(Nappi).style.cursor = "pointer"; $("#TwoOverU").animate({width: '100%', left: '0%'}, 200); $("#TwoOverUU").animate({height: '26'}, 200);};
	if (Nappi=="NappiLava") {document.getElementById(Nappi).style.cursor = "pointer"; $("#LavaOverU").animate({width: '100%', left: '0%'}, 200);};
	if (Nappi=="NappiAita") {document.getElementById(Nappi).style.cursor = "pointer"; $("#AitaOverUU, #AitaOverU").animate({height: '14'}, 200);};
	if (Nappi=="NappiNaapuri") {document.getElementById(Nappi).style.cursor = "pointer"; $("#NaapuriOverU").animate({width: '100%', left: '0%'}, 200); $("#NaapuriOverUU").animate({height: '36'}, 200);};
	
// **********
	
	if (Nappi=="NappiMailOn") {document.getElementById(Nappi).style.cursor = "pointer"; $("#MailOverU").animate({width: '100%', left: '0%'}, 200);};
