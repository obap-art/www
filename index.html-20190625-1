// ********************************************************************** 
function waitForMouseStop(callback) {
    var timer;
	function stoppedMoving(evt) {callback(); };
function moveHandler(evt) { evt = evt || window.event; 
    if (Vpla=="oon" && (timer)){window.clearTimeout(timer); 
	 $("#ylat").stop(); $("#about").stop();
	$("#ylat").animate({opacity: '1'}, 30);$("#about").animate({opacity: '0.95'}, 600); document.body.style.cursor = "default"};
    timer = window.setTimeout(function() { stoppedMoving(evt); }, 5000); };
    window.onmousemove = moveHandler;
};
waitForMouseStop(function() { if (Vpla=="oon"){$("#ylat").stop(); $("#about").stop(); $("#about").animate({opacity: '0'}, 6000);$("#ylat").animate({opacity: '0'}, 3000, function(){document.body.style.cursor = "none"});};  //$("#ylat").animate({opacity: '0.3'}, 12000); 
}); 
