// **********************************************************************  
function MClick(obj) {
	var NappiC= obj.id; sllPimennys
	
	if (NappiC=="sllPimennys") {$("#ylat").stop(); $("#about").stop(); $("#about").animate({opacity: '0.95'}, 600); $("#ylat").animate({opacity: '1'}, 10); document.body.style.cursor = "default" /*
	window.clearTimeout(timer); $("#ylat").stop(); $("#ylat").animate({opacity: '1'}, 30); document.body.style.cursor = "default"; timer = window.setTimeout(function() { stoppedMoving(evt); }, 5000); */};
	
	if (NappiC=="page-background" && Vpla=="ooff") {VMenusiirto(); };
	if (NappiC=="NappiMailOn") {$("#pimennysMail").animate({height: '512px'}, 1000); NappiMailOn.id="NappiMailOff"; $("#pimm-taustaMail").animate({top: '100%'}, 10000); InfoAni();}; //VNro=VNro-1;  VSiirto(); 
	if (NappiC=="NappiMailOff") {$("#pimennysMail").animate({height: '0%'}, 1000); NappiMailOff.id="NappiMailOn";};
	if (NappiC=="NappiMailOfff") {$("#pimennysMail").animate({height: '0%'}, 1000); NappiMailOff.id="NappiMailOn"; $("#MailOverU").animate({width: '0%', left: '100%'}, 200);};
	
	if (NappiC=="NappiInfoOn") {/*document.getElementById("obap").innerHTML = '1'*/;$("#pimennys").animate({height: '550px'}, 1000); $("#pimennysNavi").animate({height: '60px'}, 1000); NappiInfoOn.id="NappiInfoOff"; InfoM="on"; $("#pimm-tausta").stop(true).animate({top: '-1%'}, 1).animate({top: '100%'}, 10000); InfoAni();}; //VNro=VNro-1;  VSiirto(); 
	if (NappiC=="NappiInfoOff") {$("#pimennys").animate({height: '0%'}, 1000); $("#pimennysNavi").animate({height: '0%'}, 1000); NappiInfoOff.id="NappiInfoOn"; InfoM="off";};
	if (NappiC=="NappiInfoOfff") {$("#pimennys").animate({height: '0%'}, 1000); $("#pimennysNavi").animate({height: '0%'}, 1000); NappiInfoOff.id="NappiInfoOn"; $("#InfoOverU").animate({width: '0%'}, 200);InfoM="off";};
	if (NappiC=="HomeBtnInfo") {$('#pim').mCustomScrollbar('scrollTo','0');};
	if (NappiC=="OpasBtnInfo") {$('#pim').mCustomScrollbar('scrollTo','545');} 
	if (NappiC=="IdeaBtnInfo") {$('#pim').mCustomScrollbar('scrollTo','998');}
	if (NappiC=="NewsBtnInfo") {$('#pim').mCustomScrollbar('scrollTo','2610');}
	if (NappiC=="ContactBtnInfo") {$('#pim').mCustomScrollbar('scrollTo','2980');}
	if (NappiC=="DonateBtnInfo") {$('#pim').mCustomScrollbar('scrollTo','3120');}
							
	if (NappiC=="NappiMenu") {VMenusiirto() };
	if (NappiC=="NappiFullS") {document.getElementById("NappiFullS").src ="../images/ButtonsSPaus_07.png"; NappiFullS.id="NappiFullD"; FullScriin(); };
	if (NappiC=="NappiFullD") {document.getElementById("NappiFullD").src ="../images/Buttons_07.png" ; NappiFullD.id="NappiFullS"; exitFullscreen()};
	if (NappiC=="NappiSmaller") {NappiSmaller.id="NappiSmallerr"; NappiBigger.id="NappiBiggerr"; Xsize=Xsize-64; Ysize=Ysize-18; Bigger(); };
	if (NappiC=="NappiBigger") {NappiSmaller.id="NappiSmallerr"; NappiBigger.id="NappiBiggerr"; Xsize=Xsize+64; Ysize=Ysize+18; Bigger(); };  
	if (NappiC=="NappiBack") {player3.pauseVideo();VNro=VNro-1; VSiirto(); };
	if (NappiC=="NappiForward") {player3.pauseVideo();VNro=VNro+1; VSiirto(); }; 
	if (NappiC=="NappiPlay") {NappiPlay.id="NappiPlayPause"; document.getElementById("NappiPlayPause").src ="../images/ButtonsSPaus_03.png"; player3.playVideo(); };
	if (NappiC=="NappiPlayPause") {NappiPlayPause.id="NappiPlay"; document.getElementById("NappiPlay").src ="../images/Buttons_03.png";player3.pauseVideo(); };
	
	if (NappiC=="NappiRepeatAll") {document.getElementById("NappiRepeatAll").src ="../images/ButtonSuffle.png" ;NappiRepeatAll.id="NappiSuffle"; document.getElementById("NappiSuffle").title ="suffle"; RMode="3"; AutoP="On";};  
	if (NappiC=="NappiSuffle") {document.getElementById("NappiSuffle").src ="../images/ButtonPlayOne.png" ;NappiSuffle.id="NappiPlayOne"; document.getElementById("NappiPlayOne").title ="play ones"; RMode="0"; AutoP="Off";};
	if (NappiC=="NappiPlayOne") {document.getElementById("NappiPlayOne").src ="../images/ButtonRepeatOne.png" ;NappiPlayOne.id="NappiRepeatOne"; document.getElementById("NappiRepeatOne").title ="repeat one"; RMode="1"; AutoP="On";};
	if (NappiC=="NappiRepeatOne") {document.getElementById("NappiRepeatOne").src ="../images/ButtonRepeatAll.png" ;NappiRepeatOne.id="NappiRepeatAll"; document.getElementById("NappiRepeatAll").title ="repeat all"; RMode="2"; AutoP="On";};
	
	if (NappiC=="NappiSleepT") {document.getElementById("sleepss").disabled = false; $("#sleepT").animate({width: '250px'}, 300); $("#pimm-taustaSleep").animate({left: '100%'}, 10000); document.getElementById("NappiSleepT").src ="../images/ButtonSleepTof.png" ; NappiSleepT.id="NappiSleepTT"; SleepTimeri="on";}; 
	if (NappiC=="NappiSleepTT") {$("#sleepT").animate({width: '0px'}, 300); $("#ajastin").animate({right: '-360px'}, 1000); document.getElementById("NappiSleepTT").src ="../images/ButtonSleepT.png" ;NappiSleepTT.id="NappiSleepT";SleepTimeri="off";}; //clearTimeout(SleepVar); 
	
	if (NappiC=="NappiSleepOk") {SleepTimerCheck() }; 
	if (NappiC=="NappiPlayA") {VName=Noba; VNro=VNro+1; AutoP="On"; VSiirtoAni(); }; 
// *******	
	if (NappiC=="NappiNoba") {VName=Noba; VSiirtoAni(); };
	if (NappiC=="NappiAmbsu") {VName=Ambsu; VSiirtoAni(); };
	if (NappiC=="NappiKansi") {VName=Kansi; VSiirtoAni(); };
	if (NappiC=="NappiTwo") {VName=Two; VSiirtoAni(); };
	if (NappiC=="NappiLava") {VName=Lava; VSiirtoAni(); };
	if (NappiC=="NappiAita") {VName=Aita; VSiirtoAni(); }; 
	if (NappiC=="NappiNaapuri") {VName=Neighbor; VSiirtoAni(); };
	
// *******	
		if (NappiC=="slPimennys") {$("#slPimennys").stop(true);document.getElementById("slPimennys").style.width= '0px'; document.getElementById("slPimennys").style.height= '0px'; document.getElementById("slPimennys").style.opacity= '0'; document.getElementById("slPimennys").style.zIndex = '0'; document.getElementById("NappiSleepTT").src ="../images/ButtonSleepT.png" ;NappiSleepTT.id="NappiSleepT";SleepTimeri="off"; };
		
